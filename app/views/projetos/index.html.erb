<p id="notice"><%= notice %></p>

<h1><%= t '.title' %></h1>

<table>
  <thead>
    <tr>
      <th><%= t '.nome' %></th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @projetos.each do |projeto| %>
      <tr>
        <td><%= projeto.nome %></td>
        <td><%= link_to t('show'), projeto %></td>
        <td><%= link_to t('edit'), edit_projeto_path(projeto) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('new'), new_projeto_path %>