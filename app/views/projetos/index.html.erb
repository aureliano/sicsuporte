<div class="row">
  <div class="col-md-9">
    <p id="notice"><%= notice %></p>
  </div>
</div>

<div class="row">
  <div class="col-md-9">
    <h3><%= t '.title' %></h3>
  </div>
</div>

<div class="row">
  <table class="table table-striped">
    <thead>
      <tr>
        <th><%= t '.nome' %></th>
        <th colspan="2"></th>
      </tr>
    </thead>

    <tbody>
      <% @projetos.each do |projeto| %>
        <tr>
          <td width="80%"><%= projeto.nome %></td>
          <td><%= link_to t('show'), projeto, class: "btn btn-primary btn-sm", role: "button" %></td>
          <td><%= link_to t('edit'), edit_projeto_path(projeto), class: "btn btn-primary btn-sm", role: "button" %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

<br>

<div class="row">
  <%= link_to t('new'), new_projeto_path, class: "btn btn-primary", role: "button" %>
</div>